{
    "filename": "config.json",
    "content": [
      {
        "field_name": "liveMessage",
        "default": "Hey, %streamer% is live on Twitch streaming %game%! Check it out at %url% !",
        "type": "string",
        "allowEmbed": true,
        "description": "Message that gets send if the streamer goes live",
        "params": [
          {
            "name": "%streamer%",
            "description": "Name of the Streamer"
          },
          {
            "name": "%game%",
            "description": "Game which is streamed"
          },
          {
            "name": "%url%",
            "description": "Link to the stream"
          }
        ]
      },
      {
        "field_name": "liveMessageChannels",
        "default": [],
        "type": "array",
        "content": "channelID",
        "description": "ChannelID to send live-messages"
      },
      {
        "field_name": "twitchClientID",
        "default": "",
        "type": "string",
        "description": "ID of the Client, which is used to check if the Streamer is live"
      },
      {
        "field_name": "clientSecret",
        "default": "",
        "type": "string",
        "description": "Secret of the Twitch-Client, which is used to check if the Streamer is live"
      },
      {
        "field_name": "streamers",
        "default": [],
        "type": "array",
        "content": "string",
        "description": "Streamer where the notification should notify"
    },
    {
      "field_name": "interval",
      "default": 60,
      "type": "integer",
      "description": "Interval (in seconds) in which it is tested whether the streamer is live. This value must be higher than 60"
    }
  ]
}
