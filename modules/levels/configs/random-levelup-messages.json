{
  "filename": "random-levelup-messages.json",
  "configElements": true,
  "content": [
    {
      "field_name": "type",
      "default": "normal",
      "content": [
        "normal",
        "with-reward"
      ],
      "type": "select",
      "description": "Type of this message"
    },
    {
      "field_name": "message",
      "default": "",
      "allowEmbed": true,
      "type": "string",
      "description": "Messages which should be send",
      "params": [
        {
          "name": "%mention%",
          "description": "Mention of the user"
        },
        {
          "name": "%newLevel%",
          "description": "New level of the user"
        },
        {
          "name": "%role%",
          "description": "Mention of the role (No ping, only if type = with-reward)"
        }
      ]
    }
  ]
}