{
    "filename": "config.json",
    "content": [
      {
        "field_name": "live-message",
        "default": "Hey, %streamer% is live on Twitch streaming %game%! Check it out at %url%!",
        "type": "string",
        "allowEmbed": true,
        "description": "Message that gets send if the streamer goes live",
        "params": [
          {
            "name": "%streamer%",
            "description": "Name of the Streamer"
          },
          {
            "name": "%game%",
            "description": "Game which is streamed"
          },
          {
            "name": "%url%",
            "description": "Link to the stream"
          }
        ]
      },
      {
        "field_name": "live-message-channel",
        "default": "",
        "type": "channelID",
        "description": "ChannelID to send live-messages"
      },
	{
		"field_name": "Twitch-ClientID",
		"default": "",
		"type": "string",
		"description": "ID of the Client, which is used to check if the Streamer is live"
		},
		{
		"field_name": "ClientSecret",
		"default": "",
		"type": "string",
		"description": "Secret of the Client, which is used to check if the Streamer is live"
		},
      {
        "field_name": "streamer",
        "default": "",
        "type": "string",
        "description": "Streamer where the notification should notify"
    }
  ]
}